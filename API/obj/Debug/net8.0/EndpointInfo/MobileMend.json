            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ServiceCreateDTO": {
        "required": [
          "category",
          "description",
          "estimatedTime",
          "isPopular",
          "price",
          "serviceName"
        ],
        "type": "object",
        "properties": {
          "serviceName": {
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "description": {
            "minLength": 1,
            "type": "string"
          },
          "category": {
            "minLength": 1,
            "type": "string"
          },
          "estimatedTime": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "isPopular": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "TechnicianRequestCreateDTO": {
